{
  "name": "t-web-components",
  "version": "0.0.38",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/t-web-components.js",
  "es2017": "dist/esm/t-web-components.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/t-web-components/t-web-components.esm.js",
  "exports": "./dist/esm/t-web-components.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "build.watch": "stencil build --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "yarn run test:jest && yarn run test:playwright",
    "test:playwright": "npx playwright test",
    "test:playwright:ui": "npx playwright test --ui",
    "test:jest": "jest --config=jestconfig.json",
    "generate": "stencil generate",
    "deploy": "gh-pages -d www"
  },
  "dependencies": {
    "@floating-ui/dom": "1.6.5",
    "@stencil/core": "4.27.1",
    "chromator": "0.0.9",
    "highlight.js": "^11.9.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.1",
    "@stencil/react-output-target": "0.8.2",
    "@stencil/store": "2.0.16",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "gh-pages": "6.1.1",
    "jest": "29.7.0",
    "jest-cli": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.3"
  },
  "license": "MIT",
  "gitHead": "1a924a4a409ac69a5be91bcfaea9ef40523baa40"
}
